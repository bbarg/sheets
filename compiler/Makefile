OBJS =  ast.cmo three_tuple.cmo parser.cmo scanner.cmo environment.cmo \
	generator_utilties.cmo generator.cmo 

# print_ast.cmo

generator : $(OBJS)
	ocamlc -o generator $(OBJS)

# print_ast : $(OBJS)
# 	ocamlc -o print_ast $(OBJS)

.PHONY : test
test : microc testall.sh
	./testall.sh

scanner.ml : scanner.mll
	ocamllex scanner.mll

parser.ml parser.mli : parser.mly
	ocamlyacc parser.mly

%.cmo : %.ml
	ocamlc -w A -c $<

%.cmi : %.mli
	ocamlc -w A -c $<

.PHONY : clean
clean :
	rm -f parser.ml parser.mli scanner.ml \
	*.cmo *.cmi *.out *.diff generator

