#~ 
 ~ Subtracts two numbers, then returns the 
 ~ absolute value of the two 
 ~#

func int main():
    int first_num = 3
    int second_num = 4
    int absolute_val

    if (second_num > first_num):
        absolute_val = \       
            subtract(second_num, first_num)
    else if (second_num < first_num):
        absolute_val =  \
            subtract(first_num, second_num)
    else:
        return 0

    return absolute_val

func int subtract(int a, int b):
    return a - b

