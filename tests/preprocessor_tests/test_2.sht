 ~ Subtracts two numbers, then returns the 
 ~ absolute value of the two 
 ~#

int func main:
	int first_num = 3
	int second_num = 4
	int absolute_val

	if (second_num > first_num):
	    absolute_val = subtract(second_num, first_num)
	elif (second_num < first_num):
	    absolute_val =  subtract(first_num, second_num)
	else:
	    return 0
	
	return absolute_val

int func subtract(int a, int b):
	return a - b

